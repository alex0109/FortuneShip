{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "pages/*": ["app/pages/*"],
      "modules/*": ["app/modules/*"],
      "shared/*": ["app/shared/*"],
      "app/*": ["app/*"]
    }
  },
  "include": ["./app", "./declarations.d.ts"]
}
